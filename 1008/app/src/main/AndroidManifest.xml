<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eun_a.daum_map">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/hos"
        android:label="SAM"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:allowClearUserData="true"
        android:hardwareAccelerated="true"
        android:largeHeap="true">

        <activity android:name=".Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".DermatologyActivity"/>
        <activity android:name=".DentalActivity"/>
        <activity android:name=".UrologyActivity"/>
        <activity android:name=".EyeActivity"/>
        <activity android:name=".PlasticSurgeryActivity"/>
        <activity android:name=".HospitalActivity"/>
        <activity android:name=".WomansActivity"/>
        <activity android:name=".OrientalMedicineActivity"/>
        <activity android:name=".RadiologyActivity"/>
        <activity android:name=".ClinicActivity"/>
        <activity android:name=".Map"/>

    </application>

</manifest>